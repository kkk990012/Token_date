contract Token is ERC20, Ownable {
    constructor() ERC20("ROYAL", "ROYAL") {}

    using SafeMath for uint256;
    using Address for address;
    using EnumerableSet for EnumerableSet.AddressSet;

    EnumerableSet.AddressSet private _minters;
    uint256 public batchAmount = 10**18;

    mapping(address => uint256) public minterLimit;
    mapping(address => mapping(uint256 => uint256)) public recordMinted;

    event MinterAdded(address indexed minter, uint256 limit);

    function burn(uint256 amount_) external {
        _burn(_msgSender(), amount_);
    }

    function mint(address _to, uint256 _amount)
        public
        onlyMinter
        returns (bool)
    {
        if (_msgSender() != _to)
            require(!isMinter(_to), "can't mint for another minter");

        uint256 thisYear = getYear();
        address sender = _msgSender();

        if (minterLimit[sender] != 0)
            require(
                recordMinted[sender][thisYear].add(_amount) <=
                    minterLimit[sender],
                "Token: mintLimit"
            );

        _mint(_to, _amount);

        recordMinted[sender][thisYear] = recordMinted[sender][thisYear].add(
            _amount
        );

        return true;
    }

    function addMinter(address _addMinter) public onlyOwner returns (bool) {
        require(_addMinter != address(0), "_addMinter is the zero address");
        uint256 limitNum = 1 * 10**30; //default 1000 billions for owner
        if (_addMinter.isContract()) {
            require(
                IERC165(_addMinter).supportsInterface(
                    type(ITokenMinter).interfaceId
                ),
                "contract must provide the mint limit"
            );
            limitNum = ITokenMinter(_addMinter).tokenLimit();
        } else require(_addMinter == owner(), "only owner can be minter");

        minterLimit[_addMinter] = limitNum;
        emit MinterAdded(_addMinter, limitNum);
        return EnumerableSet.add(_minters, _addMinter);
    }

    function delMinter(address _delMinter) public onlyOwner returns (bool) {
        require(_delMinter != address(0), "_delMinter is the zero address");
        return EnumerableSet.remove(_minters, _delMinter);
    }

    function getMinterLength() public view returns (uint256) {
        return EnumerableSet.length(_minters);
    }

    function isMinter(address account) public view returns (bool) {
        return EnumerableSet.contains(_minters, account);
    }

    function getMinter(uint256 _index) public view onlyOwner returns (address) {
        require(_index <= getMinterLength() - 1, "index out of bounds");
        return EnumerableSet.at(_minters, _index);
    }

    function batchTransferToken(address[] memory holders) public onlyOwner {
        for (uint256 i = 0; i < holders.length; i++) {
            _transfer(_msgSender(), holders[i], batchAmount);
        }
    }

    function setBatchAmount(uint256 num) public onlyOwner {
        batchAmount = num;
    }

    modifier onlyMinter() {
        require(isMinter(_msgSender()), "caller is not the minter");
        _;
    }

    uint256 constant SECONDS_PER_DAY = 24 * 60 * 60;
    uint256 constant SECONDS_PER_HOUR = 60 * 60;
    uint256 constant SECONDS_PER_MINUTE = 60;
    int256 constant OFFSET19700101 = 2440588;

    function getYear() public view returns (uint256 year) {
        uint256 timestamp = block.timestamp;
        uint256 _days = timestamp / SECONDS_PER_DAY;
        int256 __days = int256(_days);

        int256 L = __days + 68569 + OFFSET19700101;
        int256 N = (4 * L) / 146097;
        L = L - (146097 * N + 3) / 4;
        int256 _year = (4000 * (L + 1)) / 1461001;
        L = L - (1461 * _year) / 4 + 31;
        int256 _month = (80 * L) / 2447;
        L = _month / 11;
        _month = _month + 2 - 12 * L;
        _year = 100 * (N - 49) + _year + L;

        year = uint256(_year);
    }
}
    "contracts/ROEMPToken.sol": {
      "content": "//SPDX-License-Identifier: Unlicense\npragma solidity ^0.8.0;\n\nimport \"@openzeppelin/contracts/token/ERC20/ERC20.sol\";\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\nimport \"@openzeppelin/contracts/utils/math/SafeMath.sol\";\n\nimport \"./utils/Uniswap.sol\";\n\ncontract ROEMPToken is ERC20, Ownable {\n    using SafeMath for uint256;\n\n    struct Period {\n        uint32 count;\n        uint128 price;\n        uint128 min;\n        uint128 max;\n    }\n\n    enum TranType {\n        buy,\n        sell,\n        transfer,\n        exclude\n    }\n\n    IUniswapV2Router02 public uniswapV2Router;\n    IERC20 public usdToken;\n\n    address public uniswapPair;\n\n    address public marketingWallet;\n\n    mapping(uint256 => Period) periods;\n    Period public standardPeriod;\n\n    bool public isAuthPrice = true;\n    bool public enableFee = true;\n\n    mapping(address => bool) excludeFees;\n    mapping(address => bool) excludeTxs;\n\n    constructor() ERC20(\"Rise Of Empire\", \"ROEMP\") {\n        marketingWallet = _msgSender();\n\n        _mint(_msgSender(), 100000000 * 10**18);\n        excludeFee(_msgSender());\n        excludeFee(address(this));\n        excludeFee(marketingWallet);\n        usdToken = IERC20(0x55d398326f99059fF775485246999027B3197955);\n        uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);\n        _approve(_msgSender(), address(uniswapV2Router), ~uint256(0));\n        _createPair();\n\n    }\n\n    event SyncPeriod(uint32 blockNumber, uint32 txCount, uint128 price, uint128 min, uint128 max);\n\n    function _createPair() private {\n        IUniswapV2Factory Factory = IUniswapV2Factory(uniswapV2Router.factory());\n        uniswapPair = Factory.createPair(address(this), address(usdToken));\n    }\n\n    function _transfer(\n        address from,\n        address to,\n        uint256 amount\n    ) internal override {\n        if (enableFee) {\n            uint256 feeAmount = _takeFee(from, to, amount);\n            if (feeAmount > 0) {\n                amount = amount.sub(feeAmount);\n            }\n        }\n        super._transfer(from, to, amount);\n        require(!isExcludeTx(from) && !isExcludeTx(to), \"Prohibited transactions\");\n    }\n\n    function _takeFee(\n        address from,\n        address to,\n        uint256 amount\n    ) private returns (uint256 feeAmount) {\n        TranType ftype = _getFeeType(from, to);\n        uint256 feeRate = _getTxFeeRate(ftype);\n        if (feeRate > 0) {\n            feeAmount = amount.mul(feeRate).div(100);\n            super._transfer(from, marketingWallet, feeAmount);\n            _syncPeriodPrice(getUsdPrice());\n        }\n    }\n\n    function _getFeeType(address from, address to) private view returns (TranType) {\n        if (isExcludeFee(from) || isExcludeFee(to)) {\n            return TranType.exclude;\n        } else if (from == uniswapPair) {\n            return TranType.buy;\n        } else if (to == uniswapPair) {\n            return TranType.sell;\n        } else {\n            return TranType.transfer;\n        }\n    }\n\n    function getTxFees() external view returns (uint256 buyRate, uint256 sellRate) {\n        buyRate = _getTxFeeRate(TranType.buy);\n        sellRate = _getTxFeeRate(TranType.sell);\n    }\n\n    function _getTxFeeRate(TranType feeRate) private view returns (uint256) {\n        if (feeRate == TranType.exclude) {\n            return 0;\n        } else if (feeRate == TranType.transfer) {\n            return 6;\n        }\n        uint256 usdPrice = getUsdPrice();\n        Period memory period = isAuthPrice ? getLastPeriod() : standardPeriod;\n        if (usdPrice > period.price || period.price == 0) {\n            return 6;\n        }\n        uint8[7] memory buyFees = [6, 5, 4, 3, 2, 1, 0];\n        uint8[7] memory sellFees = [6, 8, 10, 12, 14, 16, 18];\n\n        uint256 fall = uint256(period.price).sub(usdPrice);\n        uint256 rateLevel = fall.mul(100).div(period.price).div(5);\n        if (rateLevel > 6) rateLevel = 6;\n\n        return feeRate == TranType.buy ? buyFees[rateLevel] : sellFees[rateLevel];\n    }\n\n    function getUsdPrice() public view returns (uint256 price) {\n        address[] memory path = new address[](2);\n        path[0] = address(this);\n        path[1] = address(usdToken);\n\n        try uniswapV2Router.getAmountsOut(1 ether, path) returns (uint256[] memory amountOuts) {\n            price = amountOuts[1];\n        } catch {\n            price = 0;\n        }\n    }\n\n    function _getAmountIn(uint256 amount) private view returns (uint256) {\n        address[] memory path = new address[](2);\n        path[0] = address(this);\n        path[1] = address(usdToken);\n        uint256[] memory amountOuts = uniswapV2Router.getAmountsOut(amount, path);\n\n        return amountOuts[1];\n    }\n\n    function excludeFee(address addr) public onlyOwner {\n        excludeFees[addr] = true;\n    }\n\n    function manyExcludeFee(address[] memory addrs) external onlyOwner {\n        for (uint256 index = 0; index < addrs.length; index++) {\n            excludeFees[addrs[index]] = true;\n        }\n    }\n\n    function cancelExcludeFee(address[] memory addrs) external onlyOwner {\n        for (uint256 index = 0; index < addrs.length; index++) {\n            excludeFees[addrs[index]] = false;\n        }\n    }\n\n    function isExcludeFee(address addr) public view returns (bool) {\n        return excludeFees[addr];\n    }\n\n    function manyExcludeTxs(address[] memory addrs) external onlyOwner {\n        for (uint256 index = 0; index < addrs.length; index++) {\n            excludeTxs[addrs[index]] = true;\n        }\n    }\n\n    function cancelExcludeTxs(address[] memory addrs) external onlyOwner {\n        for (uint256 index = 0; index < addrs.length; index++) {\n            excludeTxs[addrs[index]] = false;\n        }\n    }\n\n    function isExcludeTx(address addr) public view returns (bool) {\n        return excludeTxs[addr];\n    }\n\n    function enableTxFee() external onlyOwner {\n        enableFee = true;\n    }\n\n    function disableTxFee() external onlyOwner {\n        enableFee = false;\n    }\n\n    function setMarketingWallet(address _marketingWallet) external onlyOwner {\n        require(_marketingWallet != address(0), \"Address cannot be empty\");\n        marketingWallet = _marketingWallet;\n    }\n\n    function setStandardPeriod(\n        uint256 price,\n        uint256 min,\n        uint256 max\n    ) external onlyOwner {\n        standardPeriod.price = uint128(price);\n        standardPeriod.min = uint128(min);\n        standardPeriod.max = uint128(max);\n        emit SyncPeriod(uint32(block.number), 0, standardPeriod.price, standardPeriod.min, standardPeriod.max);\n    }\n\n    function getCurrentPeriodKey() public view returns (uint256) {\n        return block.timestamp / 72 hours;\n    }\n\n    function getLastPeriodKey() public view returns (uint256) {\n        return (block.timestamp / 72 hours).sub(1);\n    }\n\n    function _syncPeriodPrice(uint256 price) private {\n        uint256 current = getCurrentPeriodKey();\n        Period storage item = periods[current];\n        if (item.count > 0) {\n            item.price = uint128(uint256(item.price).mul(item.count).add(price).div(item.count + 1));\n            item.count++;\n            if (price < item.min) item.min = uint128(price);\n            if (price > item.max) item.max = uint128(price);\n        } else {\n            item.price = uint128(price);\n            item.min = uint128(price);\n            item.max = uint128(price);\n\n            if (price == 0) {\n                item.count = 0;\n                //addLiquidity\n            } else {\n                item.count = 1;\n                emit SyncPeriod(uint32(block.number), item.count, item.price, item.min, item.max);\n            }\n        }\n    }\n\n    function getLastPeriod() public view returns (Period memory) {\n        return periods[getLastPeriodKey()];\n    }\n\n    function getCurrentPeriod() public view returns (Period memory) {\n        return periods[getCurrentPeriodKey()];\n    }\n\n    function enableAuthPrice() external onlyOwner {\n        isAuthPrice = true;\n    }\n\n    function disableAuthPrice() external onlyOwner {\n        isAuthPrice = false;\n    }\n\n    // prettier-ignore\n    function receiveERC20(address addr, address to, uint256 amount) external onlyOwner {\n        IERC20 token = IERC20(addr);\n        if (amount == 0) {\n            amount = token.balanceOf(address(this));\n        } \n        token.transfer(to, amount);\n    }\n\n    function receiveETH(address payable to, uint256 amount) external onlyOwner {\n        if (amount == 0) {\n            amount = payable(address(this)).balance;\n        }\n        to.transfer(amount);\n    }\n\n    receive() external payable {}\n}\n"
    },
